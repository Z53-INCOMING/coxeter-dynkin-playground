; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Coxeter Dynkin Diagram Drawer"
run/main_scene="res://coxeter_diagram_generator.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=4
window/stretch/mode="canvas_items"

[global_group]

"coxeter nodes"=""

[physics]

2d/default_gravity=0.0
2d/default_gravity_vector=Vector2(0, 0)
2d/default_linear_damp=0.3
2d/default_angular_damp=0.0

[rendering]

anti_aliasing/quality/msaa_2d=3
