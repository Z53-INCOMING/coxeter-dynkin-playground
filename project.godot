; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Coxeter Dynkin Playground"
run/main_scene="res://coxeter_diagram_generator.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=4
window/stretch/mode="canvas_items"

[global_group]

"coxeter nodes"=""
"coxeter edges"=""
coxeter=""

[physics]

common/physics_ticks_per_second=120
2d/default_gravity=0.0
2d/default_gravity_vector=Vector2(0, 0)
2d/default_linear_damp=0.5
2d/default_angular_damp=0.0
common/enable_object_picking=false

[rendering]

textures/vram_compression/import_etc2_astc=true
anti_aliasing/quality/msaa_2d=3
